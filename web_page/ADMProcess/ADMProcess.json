      "type" : "expression",
      "value" : [
        "var validate = {",
        "    \"add\":{",
        "        \"valid\":true,",
        "        \"description\":\"\"",
        "    }",
        "};",
        "",
        "if($data.appAccess.add.groupId==null || $data.appAccess.add.appId==null){",
        "    validate.add.valid = false;",
        "    validate.add.description = \"All fields are mandatory\";",
        "}",
        "",
        "var i=0;",
        "",
        "for(i=0;i<$data.listAppAccess.length;i++){",
        "    var access = $data.listAppAccess[i];",
        "    ",
        "    if(access.appId == $data.appAccess.add.appId && access.groupId == $data.appAccess.add.groupId){",
        "        validate.add.valid = false;",
        "        validate.add.description = \"Configuration already exist\";",
        "    }",
        "}",
        "",
        "return validate;"
      ],
      "exposed" : false
    },
